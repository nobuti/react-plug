{
  "extends": "standard",
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 120,
        "semi": false,
        "tabWidth": 2
      }
    ]
  },
  "plugins": ["prettier"],
  "env": {
    "jest": true
  }
}